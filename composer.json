{
  "name": "alecrabbit/php-cli-snake",
  "description": "Lightweight cli spinner with zero dependencies",
  "type": "library",
  "keywords": [
    "php",
    "spinner",
    "console",
    "cli",
    "color",
    "256color",
    "8bit",
    "colorful",
    "ansi",
    "snake"
  ],

  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "nunomaduro/collision": "^3.0",
    "react/event-loop": "^1.1",
    "symfony/var-dumper": "^4.2"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AlecRabbit\\Snake\\": "src\\Snake"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlecRabbit\\Tests\\Snake\\": "tests"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
